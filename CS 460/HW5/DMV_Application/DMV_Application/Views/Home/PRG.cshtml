
@{
    ViewBag.Title = "Thank You";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <h4><span class="label label-success">Request Submitted!</span></h4>
    </div>

    <div class="row">
        <p>Your ID is @ViewBag.CID</p>
    </div>

    <div class="row">
        <!--Go Home Button-->
        <form action="/Home" method="get">
            <div class="input-group">
                <button type="submit" class="btn btn-primary">Go Home</button>
            </div>
        </form>
    </div>
</div>